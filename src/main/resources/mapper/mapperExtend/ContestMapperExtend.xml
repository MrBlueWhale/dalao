<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ibegu.dalao.mapper.ContestMapper">

    <select id="selectPtcContestList" resultMap="BaseResultMap">
        select c.* from contest c, par_team_cts ptc where c.cid=ptc.cid and ptc.pid=#{pid}
    </select>
</mapper>